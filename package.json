{
  "name": "formularlotto",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Chaiwut Maneechot",
  "scripts": {
    "ng": "ng",
    "server:dev": "ng serve --port 1412",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:server": "ng build --aot --app 1 && npm run webpack:server",
    "build:client": "ng build --app 0 --output-hashing=false --target production --build-optimizer --vendor-chunk --no-progress && sw-precache --root=dist/browser --config=precache-config.js",
    "build:all": "npm run build:client && npm run build:server",
    "build:firebase": "rm -rf functions/server && rm -rf functions/browser && rm -rf dist/firebase-hosting && cp -a dist/browser dist/firebase-hosting && rm -rf dist/firebase-hosting/index.html && mkdir functions/browser && cp -a dist/browser/index.html functions/browser && cp -a dist/server functions/ && tsc -p ./server-firebase/tsconfig.server.json",
    "serve:firebase": "firebase serve --only functions,hosting",
    "test:unit": "ng test --progress=false --code-coverage",
    "clean:ssr": "rm -rf functions/dist-server && rm -rf functions/index.js",
    "test:lint": "npm run lint",
    "test:ci": "ng test --single-run --progress=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:firebase": "npm run build:client && npm run build:firebase && firebase deploy --only hosting"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.6",
    "@angular/cdk": "^5.2.2",
    "@angular/common": "^5.2.6",
    "@angular/compiler": "^5.2.6",
    "@angular/core": "^5.2.6",
    "@angular/flex-layout": "2.0.0-beta.10-4905443",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.6",
    "@angular/material": "^5.2.2",
    "@angular/platform-browser": "^5.2.6",
    "@angular/platform-browser-dynamic": "^5.2.6",
    "@angular/platform-server": "^5.2.6",
    "@angular/router": "^5.2.6",
    "@covalent/core": "^1.0.0",
    "@covalent/dynamic-forms": "^1.0.0",
    "@covalent/highlight": "^1.0.0",
    "@covalent/http": "^1.0.0",
    "@covalent/markdown": "^1.0.0",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "angular-l10n": "^4.0.0",
    "angular2-text-mask": "^8.0.4",
    "angularfire2": "^5.0.0-rc.5-next",
    "core-js": "^2.5.1",
    "express": "^5.0.0-alpha.6",
    "firebase": "^4.10.1",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "hammerjs": "^2.0.8",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "tslib": "^1.8.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "^5.2.6",
    "@angular/language-service": "^5.2.6",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^9.4.6",
    "codelyzer": "~4.1.0",
    "firebase-admin": "^5.5.0",
    "firebase-functions": "^0.8.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-loader": "^3.1.1",
    "ts-node": "^5.0.0",
    "tslint": "~5.9.1",
    "typescript": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  }
}
